{
  "$schema": "https://ui.shadcn.com/schema/registry-item.json",
  "name": "account-basic-info-01",
  "type": "registry:block",
  "description": "Personal information management form",
  "dependencies": [
    "date-fns"
  ],
  "registryDependencies": [
    "button",
    "card",
    "input",
    "label",
    "select",
    "calendar",
    "popover"
